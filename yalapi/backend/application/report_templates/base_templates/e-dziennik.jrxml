	<subDataset name="Lista uczestników">
		<parameter name="course_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[10]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT DISTINCT ROW_NUMBER()
        OVER (ORDER BY last_name) AS Row, first_name, last_name, email,
sex, national_identity, address_city, address_zip_code, address_street,
birth_date, birth_place, address_house_nr, address_flat_nr
FROM courses
INNER JOIN groups ON groups.id = courses.group_id
INNER JOIN group_users ON group_users.group_id = groups.id
INNER JOIN users ON users.id = group_users.user_id
LEFT JOIN user_profile ON user_profile.user_id = users.id
WHERE courses.id = $P{course_id}
ORDER BY last_name ASC, first_name ASC]]>
		</queryString>
		<field name="row" class="java.lang.Long"/>
		<field name="first_name" class="java.lang.String"/>
		<field name="last_name" class="java.lang.String"/>
		<field name="email" class="java.lang.String"/>
		<field name="sex" class="java.lang.String"/>
		<field name="national_identity" class="java.lang.String"/>
		<field name="address_city" class="java.lang.String"/>
		<field name="address_zip_code" class="java.lang.String"/>
		<field name="address_street" class="java.lang.String"/>
		<field name="birth_date" class="java.sql.Date"/>
		<field name="birth_place" class="java.lang.String"/>
		<field name="address_house_nr" class="java.lang.String"/>
		<field name="address_flat_nr" class="java.lang.String"/>
		<variable name="mieszkanie" class="java.lang.String">
			<variableExpression><![CDATA[( $F{address_flat_nr} == null ? "" : "/"+$F{address_flat_nr} )]]></variableExpression>
		</variable>
		<variable name="adres" class="java.lang.String">
			<variableExpression><![CDATA[( $F{address_street} == null ? "" : $F{address_street} )]]></variableExpression>
		</variable>
		<variable name="nr_domu" class="java.lang.String">
			<variableExpression><![CDATA[( $F{address_house_nr} == null ? "" : " "+$F{address_house_nr} )]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="Lista obecności">
		<parameter name="course_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[10]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT DISTINCT course_units.id course_unit_id, course_units.name course_unit_name,
lessons.start_date,lessons.end_date,
first_name, last_name, lesson_presence.id IS NULL present
FROM lessons
INNER JOIN course_units ON course_units.id = lessons.course_unit_id
INNER JOIN courses ON courses.id = course_units.course_id
INNER JOIN groups ON groups.id = courses.group_id
INNER JOIN group_users ON group_users.group_id = groups.id
INNER JOIN users ON users.id = group_users.user_id
LEFT JOIN lesson_presence ON lesson_presence.lesson_id = lessons.id AND lesson_presence.user_id = users.id
WHERE courses.id = $P{course_id}
ORDER BY course_units.id ASC, last_name ASC]]>
		</queryString>
		<field name="course_unit_id" class="java.lang.Integer"/>
		<field name="course_unit_name" class="java.lang.String"/>
		<field name="start_date" class="java.sql.Timestamp"/>
		<field name="end_date" class="java.sql.Timestamp"/>
		<field name="first_name" class="java.lang.String"/>
		<field name="last_name" class="java.lang.String"/>
		<field name="present" class="java.lang.Boolean"/>
	</subDataset>
	<subDataset name="Lista ocen">
		<parameter name="course_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[10]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT DISTINCT course_units.id course_unit_id, course_units.name course_unit_name,
first_name, last_name,
exams.name exam_name, exams.created_date exam_date,
exam_grades.grade
FROM lessons
INNER JOIN course_units ON course_units.id = lessons.course_unit_id
INNER JOIN courses ON courses.id = course_units.course_id
INNER JOIN groups ON groups.id = courses.group_id
INNER JOIN group_users ON group_users.group_id = groups.id
INNER JOIN users ON users.id = group_users.user_id
LEFT JOIN exams ON exams.course_unit_id = course_units.id
LEFT JOIN exam_grades ON exam_grades.exam_id = exams.id AND exam_grades.user_id = users.id
WHERE courses.id = $P{course_id}
ORDER BY course_units.id ASC, exams.created_date ASC, last_name ASC]]>
		</queryString>
		<field name="course_unit_id" class="java.lang.Integer"/>
		<field name="course_unit_name" class="java.lang.String"/>
		<field name="first_name" class="java.lang.String"/>
		<field name="last_name" class="java.lang.String"/>
		<field name="exam_name" class="java.lang.String"/>
		<field name="exam_date" class="java.sql.Date"/>
		<field name="grade" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Przebieg zajęć">
		<parameter name="course_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[10]]></defaultValueExpression>
		</parameter>
		<parameter name="date_to" class="java.lang.String">
			<defaultValueExpression><![CDATA["2013-12-31"]]></defaultValueExpression>
		</parameter>
		<parameter name="date_from" class="java.lang.String">
			<defaultValueExpression><![CDATA["1977-01-01"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
ROW_NUMBER() OVER (ORDER BY start_date) AS Row,
sum(Lessonmodules45(lessons.id)) AS  lesson_count,
CAST (start_date AS DATE) AS lesson_date,
course_units,schedule,subject,
coalesce(subject,course_units.name) AS course_unit_name,
first_name,last_name
FROM lessons
LEFT JOIN course_units ON lessons.course_unit_id=course_units.id
LEFT JOIN course_schedule ON course_schedule.lesson_id=lessons.id
LEFT JOIN users ON lessons.user_id=users.id
WHERE course_id=$P{course_id}
AND CAST (start_date AS DATE) BETWEEN CAST ($P{date_from} AS DATE) AND CAST ($P{date_to} AS DATE)
GROUP BY lesson_date,course_units,schedule,subject,start_date,course_units.name,first_name,last_name
ORDER BY start_date]]>
		</queryString>
		<field name="row" class="java.lang.Long"/>
		<field name="lesson_count" class="java.lang.Long"/>
		<field name="lesson_date" class="java.sql.Date"/>
		<field name="course_units" class="java.lang.Object"/>
		<field name="schedule" class="java.lang.String"/>
		<field name="subject" class="java.lang.String"/>
		<field name="course_unit_name" class="java.lang.String"/>
		<field name="first_name" class="java.lang.String"/>
		<field name="last_name" class="java.lang.String"/>
		<variable name="SUM" class="java.lang.Long" calculation="Sum">
			<variableExpression><![CDATA[$F{lesson_count}]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="Daty zajec">
		<parameter name="course_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
		</parameter>
		<parameter name="date_to" class="java.lang.String">
			<defaultValueExpression><![CDATA["2013-12-31"]]></defaultValueExpression>
		</parameter>
		<parameter name="date_from" class="java.lang.String">
			<defaultValueExpression><![CDATA["1977-01-01"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT MIN(CAST (start_date AS DATE)) AS min, MAX(CAST (start_date AS DATE)) AS max
FROM lessons
LEFT JOIN course_units ON lessons.course_unit_id=course_units.id
WHERE course_id=$P{course_id}
AND CAST (start_date AS DATE) BETWEEN CAST ($P{date_from} AS DATE) AND CAST ($P{date_to} AS DATE)]]>
		</queryString>
		<field name="min" class="java.sql.Date"/>
		<field name="max" class="java.sql.Date"/>
	</subDataset>
	<parameter name="course_id" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[36]]></defaultValueExpression>
	</parameter>
	<parameter name="date_to" class="java.lang.String" isForPrompting="false">
