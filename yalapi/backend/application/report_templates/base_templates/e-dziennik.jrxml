	<queryString>
		<![CDATA[SELECT courses.name course_name, courses.code course_code,
courses.description course_description, courses.start_date course_start_date, courses.end_date course_end_date,
projects.name project_name, projects.code project_code, projects.description project_description,
groups.name group_name,
training_centers.name tc_name, training_centers.street tc_street, training_centers.zip_code tc_zip_code,
training_centers.city tc_city, training_centers.phone_number tc_phone_number
FROM courses
INNER JOIN projects ON projects.id = courses.project_id
INNER JOIN groups ON groups.id = courses.group_id
INNER JOIN training_centers ON training_centers.id = courses.training_center_id
WHERE courses.id=$P{course_id}]]>
	</queryString>
