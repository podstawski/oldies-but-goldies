	<queryString>
		<![CDATA[SELECT *,gmina.name AS gmina, powiat.name AS powiat, woj.name AS woj, work_gmina.name AS work_gmina, work_powiat.name AS work_powiat, work_woj.name AS work_woj, substr(cast(age(birth_date) AS varchar),1,2) AS wiek, CAST(update_date AS date) AS data_zmiany,
extra_field(users.id,'Profil') AS f_profil,
extra_field(users.id,'Zawód/Kierunek') AS f_zawod,
extra_field(users.id,'Rok') AS f_rok
FROM user_profile LEFT JOIN users ON users.id=user_profile.user_id
LEFT JOIN poland AS gmina on user_profile.poland_id=gmina.id
LEFT JOIN poland AS powiat on gmina.parent_id=powiat.id
LEFT JOIN poland AS woj on powiat.parent_id=woj.id

LEFT JOIN poland AS work_gmina on user_profile.work_poland_id=work_gmina.id
LEFT JOIN poland AS work_powiat on work_gmina.parent_id=work_powiat.id
LEFT JOIN poland AS work_woj on work_powiat.parent_id=work_woj.id

where user_id=$P{user_id}]]>
	</queryString>
